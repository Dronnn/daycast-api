<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.daycast.caddy</string>

	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/caddy</string>
		<string>run</string>
		<string>--config</string>
